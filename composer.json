{
  "name": "galaxy-sparkle/composer-installer",
  "type": "composer-plugin",
  "description": "Composer installers plugin for various Galaxy Sparkle packages.",
  "keywords": ["galaxy-sparkle", "ecommerce", "laravel", "bootstrap"],
  "license": "MIT",
  "authors": [
    {
      "name": "zhuzhichao",
      "email": "me@zhuzhichao.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "composer-plugin-api": "^1.1"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "GalaxySparkle\\ComposerInstaller\\": "src/"
    }
  },
  "extra": {
    "class": [
      "GalaxySparkle\\ComposerInstaller\\InstallerPlugin"
    ],
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
